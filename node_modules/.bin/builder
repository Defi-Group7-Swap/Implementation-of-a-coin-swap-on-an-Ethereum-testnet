#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@nomiclabs/buidler/internal/cli/cli-with-a-typo.js" "$@"
  ret=$?
else 
  node  "$basedir/../@nomiclabs/buidler/internal/cli/cli-with-a-typo.js" "$@"
  ret=$?
fi
exit $ret
